<div class="author">
  <h1 class="author__heading">My Profile</h1>
  <img src="/img/profile-image-girl.jpg" alt="girl working on her computer" class="profile-image">
</div>



<div class="admin-title">
  <h2>My Posts:</h2>
  <a href="/add-post" class="button">+ Add New</a>
</div>

<ul class="admin-posts">

  <% data.forEach(post => { %>
   
    <li>
      <a href="/post/<%= post._id %>">
        <%= post.title %> &nearr;
      </a>
      <div class="admin-post-controls">
        <a href="/edit-post/<%= post._id %>" class="btn">Edit</a>

        <form action="/delete-post/<%= post._id %>?_method=DELETE" method="POST">
          <input type="submit" value="Delete" class="btn-delete btn">
        </form>

      </div>

    </li>

  <% }) %>
</ul>